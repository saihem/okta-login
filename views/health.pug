extends layout
 
block variables
  - var title = "Health"
 
block content
  h2.text-center Welcome to your health dashboard #{desc.name}! 
 
  .row
    .offset-sm-2.col-sm-8
      .jumbotron.text-center
        p(style='white-space:pre;').
          Here is your #[strong medical patient] data:
        table.table.table-striped
            each val, item in desc
                tr
                    td
                        strong= item
                    td= val
